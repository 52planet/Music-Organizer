@model IEnumerable<F2021A5LB.Models.ArtistBaseViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Artist List</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.BirthName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BirthOrStartDate)
        </th>
      
        <th>
            @Html.DisplayNameFor(model => model.Genre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UrlArtist)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.BirthName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BirthOrStartDate)
        </td>
       
        <td>
            @Html.DisplayFor(modelItem => item.Genre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
           
            <img src= @Html.DisplayFor(modelItem => item.UrlArtist) width="100" height="100">
        </td>
        <td>
            
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
           
        </td>
    </tr>
}

</table>
